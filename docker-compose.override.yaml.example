services:
  db:
    volumes:
      - './volumes/db:/var/lib/postgresql/data'
    ports:
      - '${DB_PORT_HOST}:${DB_PORT_CONTAINER}'

  cache:
    volumes:
      - './volumes/cache:/data'
    ports:
      - '${CACHE_PORT_HOST}:${CACHE_PORT_CONTAINER}'

  greeter:
    environment:
      NODE_ENV: development
    ports:
      - '${GREETER_PORT_HOST}:${GREETER_PORT_CONTAINER}'

  server:
    environment:
      NODE_ENV: development
