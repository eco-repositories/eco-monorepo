{
  "name": "shared",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#utils/*": "@libs/eco__utils/*",
    "#@/*": {
      "@parzh/map-src-to-dist": "./dist/*",
      "default": "./src/*"
    }
  },
  "exports": {
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "test": "vitest",
