{
  "name": "shared",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#utils/*": "../../libs/@eco/utils/packages/utils/*",
    "#@/*": "./src/*"
  },
  "exports": {
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "lint": "eslint \"**/*.ts\"",
    "clean": "rm -rf dist/* ./*.tsbuildinfo",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json"
  },
  "devDependencies": {
    "@eslint/eslintrc": "*",
    "@eslint/js": "*",
    "@nestjs/terminus": "11.0.0",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-promise": "7.2.1",
    "typescript": "*",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.1.6",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1"
  }
}
