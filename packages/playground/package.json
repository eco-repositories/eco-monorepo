{
  "name": "playground",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": null,
  "scripts": {
    "__touch_entry": "touch src/index.ts",
    "__clean_dist": "rm -rf dist/*",
    "prebuild": "npm run __touch_entry && npm run __clean_dist",
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "tsc-alias --project tsconfig.build.json",
    "prestart": "npm run build",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/node": "18.16.16",
    "tsc-alias": "1.8.7",
    "typescript": "5.2.2"
  }
}
