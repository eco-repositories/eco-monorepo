{
  "name": "playground",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#@/*": "./src/*"
  },
  "exports": null,
  "scripts": {
    "predev": "touch src/index.ts",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf ./dist/*",
    "resolve-path-aliases": "tsc-alias --project tsconfig.json",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.json",
    "postbuild": "npm run resolve-path-aliases"
  },
  "devDependencies": {
    "@libs--eco/utils": "file:../../libs/@eco/utils/packages/utils",
    "@types/node": "*",
    "shared": "file:../shared",
    "tsc-alias": "1.8.10",
    "tsx": "4.19.2",
    "typescript": "*"
  }
}
