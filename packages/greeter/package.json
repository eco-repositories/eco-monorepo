{
  "name": "greeter",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#utils/*": "@libs/eco__utils/*",
    "#shared/*": "shared/*",
    "#@/*": {
      "@parzh/map-src-to-dist": "./dist/*",
      "default": "./src/*"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build --verbose",
    "start": "node --enable-source-maps --conditions=@parzh/map-src-to-dist dist/main.js",
    "lint": "eslint \"**/*.ts\"",
    "pretest": "npm run -w shared build",
    "test": "vitest"
  },
  "dependencies": {
    "@libs/eco__utils": "file:../../libs/@eco/utils/packages/utils",
    "@nestjs/common": "11.1.6",
    "@nestjs/core": "11.1.6",
    "@nestjs/microservices": "11.1.6",
    "class-validator": "0.14.1",
    "shared": "file:../shared"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.35.0",
    "@nestjs/schematics": "11.0.7",
    "@types/node": "22.18.6",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "eslint": "9.35.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.0",
    "eslint-plugin-promise": "7.2.1",
    "typescript": "5.9.2",
    "unplugin-swc": "1.5.7",
    "vitest": "3.2.4"
  }
}
