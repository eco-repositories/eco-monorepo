{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "nest build",
    "postbuild": "tsc-alias --project tsconfig.build.json",
    "start": "node --enable-source-maps dist/main.js",
    "dev": "nest start --watch",
    "debug": "npm run dev -- --debug",
    "lint": "eslint \"**/*.ts\"",
    "pretest": "npm run -w shared build",
    "test": "vitest",
    "_typeorm-cli": "tsx ./meta/typeorm-tsx-esm-cli.ts",
    "_orm": "npm run _typeorm-cli -- --dataSource ./src/db/data-source/data-source-instance.ts",
    "premigration:generate": "echo Ensure that the database is running and the file \u001b[1mpackages/server/.env.local\u001b[0m exists",
    "migration:generate": "npm run _orm -- migration:generate ./src/db/migrations/${npm_config_name:-migrate}",
    "migration:run": "npm run _orm -- migration:run"
  },
  "dependencies": {
    "@keyv/redis": "5.1.1",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.1.6",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/typeorm": "11.0.0",
    "cache-manager": "7.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "express-rate-limit": "7.5.0",
    "morgan": "1.10.1",
    "object-hash": "^3.0.0",
    "pg": "8.13.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "0.2.19",
    "@eslint/eslintrc": "*",
    "@eslint/js": "*",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.7",
    "@nestjs/testing": "11.1.6",
    "@types/express": "5.0.0",
    "@types/morgan": "1.9.9",
    "@types/node": "*",
    "@types/object-hash": "3.0.6",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-promise": "7.2.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.10",
    "tsx": "4.19.2",
    "typescript": "*",
    "vitest": "3.0.6"
  }
}
