{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#utils/*": "@libs/eco__utils/*",
    "#shared/*": "shared/*",
    "#@/*": {
      "@parzh/map-src-to-dist": "./dist/*",
      "default": "./src/*"
    }
  },
  "scripts": {
    "build": "tsc --build --verbose",
    "start": "node --enable-source-maps --conditions=@parzh/map-src-to-dist dist/main.js",
    "_sorry_no_watch_mode": "echo \u001b[31mError\u001b[0m: Package \u001b[1mtsconfig-paths\u001b[0m, used by nest --watch, \u001b[1mdoes not support ESM\u001b[0m && exit 1",
    "dev": "npm run --silent _sorry_no_watch_mode && nest start --watch",
    "debug": "npm run --silent _sorry_no_watch_mode && nest start --watch --debug",
    "lint": "eslint \"**/*.ts\"",
    "pretest": "npm run -w shared build",
    "test": "vitest",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "_typeorm-cli": "tsx ./meta/typeorm-tsx-esm-cli.ts",
    "_orm": "npm run _typeorm-cli -- --dataSource ./src/db/data-source/data-source-instance.ts",
    "premigration:generate": "echo Ensure that the database is running and the file \u001b[1mpackages/server/.env.local\u001b[0m exists",
    "migration:generate": "npm run _orm -- migration:generate ./src/db/migrations/${npm_config_name:-migrate}",
    "migration:run": "npm run _orm -- migration:run"
  },
  "dependencies": {
    "@keyv/redis": "5.1.1",
    "@libs/eco__utils": "file:../../libs/@eco/utils/packages/utils",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.1.6",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/typeorm": "11.0.0",
    "cache-manager": "7.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "express-rate-limit": "7.5.0",
    "morgan": "1.10.1",
    "pg": "8.13.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "shared": "file:../shared",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@eslint/eslintrc": "*",
    "@eslint/js": "*",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.7",
    "@nestjs/testing": "11.1.6",
    "@swc/core": "1.13.5",
    "@types/express": "5.0.0",
    "@types/morgan": "1.9.9",
    "@types/node": "*",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-n": "*",
    "eslint-plugin-promise": "*",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "tsx": "4.20.5",
    "typescript": "*",
    "unplugin-swc": "1.5.7",
    "vitest": "3.2.4"
  }
}
