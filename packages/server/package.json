{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "nest build",
    "postbuild": "tsc-alias --project tsconfig.build.json",
    "start": "node --enable-source-maps dist/main.js",
    "_sorry_no_watch_mode": "echo \u001b[31mError\u001b[0m: Package \u001b[1mtsconfig-paths\u001b[0m, used by nest --watch, \u001b[1mdoes not support ESM\u001b[0m && exit 1",
    "dev": "npm run --silent _sorry_no_watch_mode && nest start --watch",
    "debug": "npm run --silent _sorry_no_watch_mode && nest start --watch --debug",
    "lint": "eslint \"src/**/*.ts\"",
    "pretest": "npm run -w shared build",
    "test": "vitest"
  },
  "dependencies": {
    "@nestjs/common": "9.4.2",
    "@nestjs/config": "2.3.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/platform-express": "9.4.2",
    "@nestjs/sequelize": "10.0.0",
    "@nestjs/terminus": "^9.2.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express-rate-limit": "6.7.0",
    "morgan": "1.10.0",
    "object-hash": "^3.0.0",
    "pg": "8.11.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sequelize": "6.32.1",
    "sequelize-typescript": "2.1.5"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "0.2.19",
    "@nestjs/cli": "10.1.12",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.2",
    "@types/express": "4.17.17",
    "@types/morgan": "1.9.4",
    "@types/node": "18.16.16",
    "@types/object-hash": "^3.0.2",
    "@types/sequelize": "4.28.15",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.41.0",
    "eslint-config-standard-with-typescript": "35.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.6",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "vitest": "0.31.1"
  }
}
